    "    \"        acc = data[\\\"primaryAccession\\\"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"        frags = []\\n\",\n",
    "    \"        for f in parsed_frags:\\n\",\n",
    "    \"            if f[\\\"start\\\"] == 0:\\n\",\n",
    "    \"                f_type = \\\"n-term\\\"\\n\",\n",
    "    \"            elif f[\\\"stop\\\"] == len(seq):\\n\",\n",
    "    \"                f_type = \\\"c-term\\\"\\n\",\n",
    "    \"            else:\\n\",\n",
    "    \"                f_type = \\\"internal\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"            frags.append((acc, f_type, seq[f[\\\"start\\\"]:f[\\\"stop\\\"]], ))\\n\",\n",
    "    \"\\n\",\n",
    "    \"        if multi_frag:\\n\",\n",
    "    \"            raise \\\"Not currently Implemented\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"        return frags\\n\",\n",
    "    \"    except Exception:\\n\",\n",
    "    \"        return None, None, None\"\n",
