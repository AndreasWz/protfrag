    "    \"def generate_fragments(data, fragment_types, multi_frag=False):\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        parsed_frags = [{\\\"type\\\": e[\\\"type\\\"], \\\"start\\\": int(e[\\\"location\\\"][\\\"start\\\"][\\\"value\\\"]) - 1, \\\"stop\\\": int(e[\\\"location\\\"][\\\"end\\\"][\\\"value\\\"])} for e in record[\\\"features\\\"] if e[\\\"type\\\"] in fragment_types]\\n\",\n",
